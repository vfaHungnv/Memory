#ifdef __OBJC__
#import "iDefine.h"
#endif

#ifdef DEBUG
#define DEVLOG(fmt, ...)  NSLog((@"%s [Line %d] " fmt), __PRETTY_FUNCTION__, __LINE__, ##__VA_ARGS__)
